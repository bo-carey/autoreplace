{
  "name": "Backstabbr Assistant",
  "version": "0.0.0",
  "description": "A browser extension to facilitate games of backstabbr.",
  "homepage_url": "https://github.com/bo-carey/backstabbr-assistant",
  "manifest_version": 2,
  "minimum_chrome_version": "74",
  "icons": {
    "16": "./images/tank-16.png",
    "24": "./images/tank-24.png",
    "32": "./images/tank-32.png",
    "48": "./images/tank-48.png",
    "64": "./images/tank-64.png",
    "128": "./images/tank-128.png"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["*://*.backstabbr.com/game/*"],
      "js": ["index.js"],
      "run_at": "document_end"
    }
  ]
}
