{
  "name": "Backstabbr Assistant",
  "version": "0.0.0",
  "description": "A browser extension to facilitate games of backstabbr.",
  "homepage_url": "https://github.com/bo-carey/backstabbr-assistant",
  "manifest_version": 2,
  "minimum_chrome_version": "74",
  "icons": {
    "16": "./icons/16.png",
    "24": "./icons/24.png",
    "32": "./icons/32.png",
    "48": "./icons/48.png",
    "64": "./icons/64.png",
    "128": "./icons/128.png"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["*://*.backstabbr.com/game/*"],
      "js": ["main.js"],
      "run_at": "document_idle"
    }
  ]
}
